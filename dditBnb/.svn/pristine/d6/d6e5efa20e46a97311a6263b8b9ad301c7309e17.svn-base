	public String getProd_part() {
		return prod_part;
	}
	public void setProd_part(String prod_part) {
		this.prod_part = prod_part;
	}
	public String getProd_add1() {
		return prod_add1;
	}
	public void setProd_add1(String prod_add1) {
		this.prod_add1 = prod_add1;
