<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<%
	String addr = request.getParameter("addr");
	String check_in = request.getParameter("check_in");
	String check_out = request.getParameter("check_out");
	int person = Integer.parseInt(request.getParameter("person"));
%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>대덕비앤비:검색 결과</title>
<script type="text/javascript" src="../js/jquery-3.6.0.min.js"></script>
<link rel="stylesheet" href="../style/reset.css">
<link href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet">
<link href="https://fonts.googleapis.com/css2?family=Black+Han+Sans&display=swap" rel="stylesheet">
<link href="https://fonts.googleapis.com/css2?family=Fira+Sans:ital,wght@1,900&display=swap" rel="stylesheet">
<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/css/bootstrap.min.css">
<script src="https://cdn.jsdelivr.net/npm/jquery@3.6.0/dist/jquery.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.1/dist/umd/popper.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/bootstrap@4.6.1/dist/js/bootstrap.bundle.min.js"></script>
<script src="../js/searchProd.js"></script>
<script src="../js/util.js"></script>
<script type="text/javascript" src="../js/signUp.js"></script>
<script type="text/javascript" src="../js/login.js"></script>
<link href="../style/searchProdStyle.css" rel="stylesheet">
<link href="../datepicker/css/datepicker.min.css" rel="stylesheet" type="text/css" media="all">
<!-- Air datepicker css -->
<script src="../datepicker/js/datepicker.js"></script> <!-- Air datepicker js -->
<script src="../datepicker/js/i18n/datepicker.ko.js"></script> <!-- 달력 한글 추가를 위해 커스텀 -->
<script type="text/javascript" src="https://openapi.map.naver.com/openapi/v3/maps.js?ncpClientId=r0jeev2fdl&submodules=geocoder"></script>
<%
	Cookie[] cookies = request.getCookies();
	
	String user = "";
	String check = "";
	
	if(cookies != null){
		for(Cookie cookie : cookies){
			if(cookie.getName().equals("user")){
				user = cookie.getValue();
			}
		}
	}
%>
<script type="text/javascript">
$(function(){
	prodObj = {}
	prodObj.addr = "<%= addr%>";
	if(!<%=check_in.equals("")%>) prodObj.check_in = "<%=check_in%>";
	if(!<%=check_out.equals("")%>) prodObj.check_out = "<%=check_out%>";
	if(<%=person > 0%>) prodObj.person = <%=person%>;
	

	
	const loadProd = (obj) => {
		$.ajax({
			url : "/dditBnb/prodSearch.do",
			type : "post",
			data : obj,
			success : function(res){
				
				var map = new naver.maps.Map("map", {
					  center :setMapCenter($('#addr').text()),
					  zoom: 11,
					  mapTypeControl: true
					});
				
				map.setCursor('pointer');
					
				function setMapCenter(address) {
					  naver.maps.Service.geocode({
					    query: address
					  }, function(status, response) {
					    if (status === naver.maps.Service.Status.ERROR) {
					      if (!address) {
					        return alert('Geocode Error, Please check address');
					      }
					      return alert('Geocode Error, address:' + address);
					    }
	
					    if (response.v2.meta.totalCount === 0) {
					      return alert('No result.');
					    }
	
					    
				     	var item = response.v2.addresses[0],
					    	point = new naver.maps.Point(item.x, item.y);
	
				     	
				     	map.setCenter(point);
					   	
					  });
					}
				
				code = "";
				$.each(res, function(i,v){
					
										
						code += '<div class="searchItem" id="' + v.prod_id + '">'
						code += '	<div class="itemImage">'
						code += '		<img src="' + v.link[0] + '">'	
						code += '	</div>'
						code += '	<div class="itemInfo">'
						code += '		<div class="itemInfo_title">'
						code += '			<p class="item_type"><span>' + v.prod_type + '</span>'
						code += '				<span class="addFavorite">'
						code += '					<svg viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" role="presentation" focusable="false" style="display: block; fill: transparent; height: 24px; width: 24px; stroke: black; stroke-width: 2; overflow: visible;">'
						code += ' 						<path d="m16 28c7-4.733 14-10 14-17 0-1.792-.683-3.583-2.05-4.95-1.367-1.366-3.158-2.05-4.95-2.05-1.791 0-3.583.684-4.949 2.05l-2.051 2.051-2.05-2.051c-1.367-1.366-3.158-2.05-4.95-2.05-1.791 0-3.583.684-4.949 2.05-1.367 1.367-2.051 3.158-2.051 4.95 0 7 7 12.267 14 17z">'
						code += '						</path>'		
						code += '					</svg>'	
						code += '				</span>'
						code += '			</p>'
						code += '			<p class="item_name">' + v.prod_name + '</p>'
						code += '		</div><hr>'
						code += '		<div class="itemInfo_des">'
						code += '       	<div class="PerConven">'
						code += '				<p>최대 인원 ' + v.prod_per + '명 '
						if(v.convenVo.length > 0){							
							for(i = 0; i < v.convenVo.length; i++){
								if(i > 0) code += "/";
								code += v.convenVo[i].conven_name
							} 
						}
						code += '				</p>'
						code += '			</div>'
						code += '			<div class="starPrice">'	
						code += ' 				<div class="review">'
						code += '					<span class="star">'
						code += '						<svg viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" role="presentation" focusable="false" style="display: block; fill: #FF385C; height: 14px; width: 14px;">'
						code += ' 							<path d="M15.094 1.579l-4.124 8.885-9.86 1.27a1 1 0 0 0-.542 1.736l7.293 6.565-1.965 9.852a1 1 0 0 0 1.483 1.061L16 25.951l8.625 4.997a1 1 0 0 0 1.482-1.06l-1.965-9.853 7.293-6.565a1 1 0 0 0-.541-1.735l-9.86-1.271-4.127-8.885a1 1 0 0 0-1.814 0z" fill-rule="evenodd">'
						code += '							</path>'		
						code += '						</svg>'	
						code += '					</span>'
						code += '					<span class="starNum">'
						code += v.star 
						code += '					</span>'
						code += ' 				</div>'
						code += '				<div class="price">'
						code += '					<p class="oneDay">'
						code += '₩' + priceToString(v.prod_price) + ' / 박'
						code += '					</p>'
						code += '				</div>'
						code += '			</div>'	
						code += '		</div>'
						code += '	</div>'
						code += '</div>	'
						code += '<hr>'
						
						markerAddr =  v.prod_add1 + v.prod_add2
						
						var marker = new naver.maps.Marker({
							position : getAddrToMarker(markerAddr),
						    map: map
						});
						
						function getAddrToMarker(address){
							naver.maps.Service.geocode({
							    query: address
							  }, function(status, response) {
							    if (status === naver.maps.Service.Status.ERROR) {
							      if (!address) {
							        return alert('Geocode Error, Please check address');
							      }
							      return alert('Geocode Error, address:' + address);
							    }
	
							   /*  if (response.v2.meta.totalCount === 0) {
							      return alert('No result.');
							    } */
	
							    
						     	var item = response.v2.addresses[0],
							    	point = new naver.maps.Point(item.x, item.y);
	
						     	
						     	marker.setPosition(point);
							   	
							  });
						}
						
						var markerContent = "";
							markerContent += '<div class="markerContent">'
							markerContent += '	<div class="contentImg">'
							markerContent += '		<img src="' + v.link[0] + '">'
							markerContent += '	</div>'
							markerContent += '	<div class="contentDes">'
							markerContent += '		<p class="markerReview">'
							markerContent += '			<span class="start">'								
							markerContent += '				<svg viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg" aria-hidden="true" role="presentation" focusable="false" style="display: block; fill: #FF385C; height: 14px; width: 14px;">'
							markerContent += '					<path d="M15.094 1.579l-4.124 8.885-9.86 1.27a1 1 0 0 0-.542 1.736l7.293 6.565-1.965 9.852a1 1 0 0 0 1.483 1.061L16 25.951l8.625 4.997a1 1 0 0 0 1.482-1.06l-1.965-9.853 7.293-6.565a1 1 0 0 0-.541-1.735l-9.86-1.271-4.127-8.885a1 1 0 0 0-1.814 0z" fill-rule="evenodd">'
							markerContent += '					</path>'
							markerContent += '				</svg>'
							markerContent += '			</span>'
							markerContent += '			<span class="starNum"> '+ v.star +' </span>'
							markerContent += '		</p>'
							markerContent += '		<p class="markerProdName">' + v.prod_name + '</p>'
							markerContent += '	</div>'
							markerContent += '</div>'
						
						
						var infowindow = new naver.maps.InfoWindow({
							content : markerContent,
							maxWidth : 250,
							borderWidth:0,
							disableAnchor :false
							
						})
						
						naver.maps.Event.addListener(marker, "click", function(e) {
						    if (infowindow.getMap()) {
						        infowindow.close();
						    } else {
						        infowindow.open(map, marker);
						    }
						});
					
				})
		
			
				$('#searchList').html(code);
			
			},
			error : function(xhr){
				alert("상태 : " + xhr.status)
			},
			dataType : 'json'
		})	
	}
	loadProd(prodObj);
	
	$('#savePrice').click(function(){
		prodObj.min_price = $('#input-left').val();
		prodObj.max_price = $('#input-right').val();
		
		loadProd(prodObj);
	})
	
	$('#saveType').click(function(){
		prodObj.part = $('input[type=radio]:checked').val();
		
		loadProd(prodObj);
	})
	
	$('#sort_price_desc').click(function(){
		prodObj.sort = "prod_price";
		prodObj.order = "asc";
		
		loadProd(prodObj);
	})
	
	$('#sort_price_asc').click(function(){
		prodObj.sort = "prod_price";
		prodObj.order = "desc";
		
		loadProd(prodObj);
	})
	
	$('#sort_star').click(function(){
		prodObj.sort = "star";
		prodObj.order = "asc";
		
		loadProd(prodObj);
	})
	
	function priceToString(price) {
    	return price.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ',');
	}
	
	$('#where').val($('#addr').text());
	
	$('main').on('click', '.addFavorite', function(){
		if("<%= user%>" == ""){
			alert("로그인 후 이용해주세요.")
		}else{		
			$('path', this).attr("fill","#FF385C")
			$.ajax({
				url : '/dditBnb/addFavorite.do',
				type : 'post',
				data : {
					"prod_id" : $(this).parents('.searchItem').attr("id"),
					"mem_id" : "<%= user%>"
				},
				success : function(res){
					alert(res);
				},
				error : function(xhr){
					alert("상태 : " + xhr.status)
				}
			}) 
		}
	})
	
	$('main').on('click', '.itemImage', function(){
		location.href = "../main/prodDetail.jsp?prodId=" + $(this).parent('.searchItem').attr("id") + "&check_in=<%= check_in%>&check_out=<%= check_out%>&person=<%= person%>";
	})
	
})
</script>
</head>
<body>
<header>
	<div id="headerTop">
		<div id="logoContainer">
			<a href="./index.jsp">
				<svg width="35" height="32" fill="hotpink" style="display:block;">
					<path d="
						M 16 1 c 2.008 0 3.463 0.963 4.751 3.269 l 0.533 1.025 c 1.954 3.83 6.114 12.54 7.1 14.836 l 0.145 0.353 c 0.667 1.591 0.91 2.472 0.96 3.396 l 0.01 0.415 l 0.001 0.228 c 0 4.062 -2.877 6.478 -6.357 6.478 c -2.224 0 -4.556 -1.258 -6.709 -3.386 l -0.257 -0.26 l -0.172 -0.179 h -0.011 l -0.176 0.185 c -2.044 2.1 -4.267 3.42 -6.414 3.615 l -0.28 0.019 l -0.267 0.006 C 5.377 31 2.5 28.584 2.5 24.522 l 0.005 -0.469 c 0.026 -0.928 0.23 -1.768 0.83 -3.244 l 0.216 -0.524 c 0.966 -2.298 6.083 -12.989 7.707 -16.034 C 12.537 1.963 13.992 1 16 1 Z m 0 2 c -1.239 0 -2.053 0.539 -2.987 2.21 l -0.523 1.008 c -1.926 3.776 -6.06 12.43 -7.031 14.692 l -0.345 0.836 c -0.427 1.071 -0.573 1.655 -0.605 2.24 l -0.009 0.33 v 0.206 C 4.5 27.395 6.411 29 8.857 29 c 1.773 0 3.87 -1.236 5.831 -3.354 c -2.295 -2.938 -3.855 -6.45 -3.855 -8.91 c 0 -2.913 1.933 -5.386 5.178 -5.42 c 3.223 0.034 5.156 2.507 5.156 5.42 c 0 2.456 -1.555 5.96 -3.855 8.907 C 19.277 27.766 21.37 29 23.142 29 c 2.447 0 4.358 -1.605 4.358 -4.478 l -0.004 -0.411 c -0.019 -0.672 -0.17 -1.296 -0.714 -2.62 l -0.248 -0.6 c -1.065 -2.478 -5.993 -12.768 -7.538 -15.664 C 18.053 3.539 17.24 3 16 3 Z m 0.01 10.316 c -2.01 0.021 -3.177 1.514 -3.177 3.42 c 0 1.797 1.18 4.58 2.955 7.044 l 0.21 0.287 l 0.174 -0.234 c 1.73 -2.385 2.898 -5.066 2.989 -6.875 l 0.006 -0.221 c 0 -1.906 -1.167 -3.4 -3.156 -3.421 h -0.001 Z
					">
					</path>
				</svg>
				<span id="logoTitle">
					dditBnB
				</span>
			</a>
		</div>
		<div id="filterSearchForm">
			<div id="filterSearch_toggle">
				<button>
					<span id="addr"><%= addr%></span>
				</button>
				<button>
					<%
						if(!check_in.equals("") && !check_out.equals("")){
							SimpleDateFormat stringToDate = new SimpleDateFormat("yyyy-MM-dd");
							Date checkIn = stringToDate.parse(check_in);
							Date checkOut = stringToDate.parse(check_out);
							
							SimpleDateFormat dateToString = new SimpleDateFormat("MM월 dd일");
							String formatCheckIn = dateToString.format(checkIn);
							String formatCheckOut = dateToString.format(checkOut);
							
					%>
						<span>
							<%= formatCheckIn%> ~ <%= formatCheckOut %>
						</span>
					<%		
						}else{
					%>
						<span> 날짜 입력</span>
					<%		
						}
					%>
				</button>
				<button>
					<%
						if(person > 0){
					%>
						<span id="">게스트 <%= person %>명</span>
					<%		
						}else{
					%>
						<span>인원 입력</span>
					<%		
						}
					%>
				</button>
				<button class="searchBtn">
						<span class="material-icons">
							search
						</span>
				</button>
			</div>
			<div class="searchBar">
				<div class="searchInput searchWhere">
					<p>위치</p>
					<input type="text" placeholder="어디로 여행가세요?" id="where" autocomplete="off">
				</div>
				<div id="searchHistory" style="display: none;">
					<p style="margin-top: 20px; margin-left:30px; margin-bottom:0; font-weight: bold;">
