<script type="text/javascript">

$(function(){
	$('#findIdBtn').on('click', function(){		
		$.ajax({
			url: '/dditBnb/findID.do',
			type:'post',
			data : {
				"name" : $('#findIdname').val(),
				"tel" : $('#findIdtel').val()	
			},
			success: function(res){			
				code = "";		
				
				if(res == null) {
					code += "<p>아이디가 존재하지 않습니다.</p>";
				} else {
					code += "<p>찾으시는 아이디는 " + res + " 입니다.</p>";
				}			
			
				$('#findIdResult').html(code);			
			},
			error : function(xhr){
				alert("상태 : " + xhr.status);
			},
			dataType:'json'
		})	
		
	})
	
	$('#findId').click(function(){
		$('#findIdname').val("");
		$('#findIdtel').val("");
		$('#findIdResult').html("");
	})
	
	$('#findPwdBtn').on('click', function(){		
		$.ajax({
			url: '/dditBnb/findPass.do',
			type:'post',
			data : {
				"id" : $('#findPassid').val(),
				"name" : $('#findPassname').val(),	
				"hint" : $('#findPasshint').val(),
				"ans" : $('#findPassans').val()
			},
			success: function(res){					
					alert(res);	
					if(res == "회원정보가 확인됐습니다."){						
						$('#newPassForm').modal('show');
						$('#findPwdForm').modal('hide');
					}				
			},
			error : function(xhr){
				alert("상태 : " + xhr.status);
			}
		})	
		
	})
	
	$('#findPass').click(function(){
		$('#findPassid').val("");
		$('#findPassname').val("");
		$('#findPassans').val("");
	})
	
	$('#getnewpass').on('click', function(){		
		$.ajax({
			url: '/dditBnb/updatePass.do',
			type:'post',
			data : {
				"id" : $('#findPassid').val(),
				"pass" : $('#newpass').val()
			},
			success: function(res){	
				alert('비밀번호가 수정되었습니다.');	
				$('#newPassForm').modal('hide');		
			},
			error : function(xhr){
				alert("상태 : " + xhr.status);
			},
			dataType:'json'
		})	
		
	})
	
})
</script>
